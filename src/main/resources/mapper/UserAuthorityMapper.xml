<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.felix.security.jwt.mapper.UserAuthorityMapper">

    <select id="findUserAuthorityById" parameterType="long" resultType="com.felix.security.jwt.entity.UserAuthority">
      select id, name from user_authority where id = #{id}
    </select>

</mapper>